 - name: web
   type: html5
   path: web
   requires:
      - name: openSAPHANA5_<group number>-uaa
      - name: ui5-lib
        properties:
          ui5liburl: ~{url}
      - name: core-js-service
        group: destinations
        properties:
          name: core-backend
          url: ~{url}
          forwardAuthToken: true 
      - name: user-js-service
        group: destinations
        properties:
          name: user-backend
          url: ~{url}
          forwardAuthToken: true 
